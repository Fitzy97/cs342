<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_lunch"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.user.assignment_1.LunchActivity">

    <TextView
        android:id="@+id/lunch_title_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@string/lunch_title_text"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/lunch_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lunch_title_text"/>

    <TextView
        android:id="@+id/lunch_title_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@string/lunch_title_text2"
        android:layout_below="@+id/lunch_text"/>

    <TextView
        android:id="@+id/lunch_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lunch_title_text2"/>

    <TextView
        android:id="@+id/lunch_title_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@string/lunch_title_text3"
        android:layout_below="@+id/lunch_text2"/>

    <TextView
        android:id="@+id/lunch_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lunch_title_text3"/>

    <TextView
        android:id="@+id/lunch_title_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@string/lunch_title_text4"
        android:layout_below="@+id/lunch_text3"/>

    <TextView
        android:id="@+id/lunch_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lunch_title_text4"/>

    <TextView
        android:id="@+id/lunch_title_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@string/lunch_title_text5"
        android:layout_below="@+id/lunch_text4"/>

    <TextView
        android:id="@+id/lunch_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lunch_title_text5"/>

</RelativeLayout>